    console.error('Uncaught Exception:', error);
    // åœ¨æµ‹è¯•çŽ¯å¢ƒä¸­ï¼Œè®©æœªå¤„ç†çš„å¼‚å¸¸å¯¼è‡´æµ‹è¯•å¤±è´¥
    throw error;
