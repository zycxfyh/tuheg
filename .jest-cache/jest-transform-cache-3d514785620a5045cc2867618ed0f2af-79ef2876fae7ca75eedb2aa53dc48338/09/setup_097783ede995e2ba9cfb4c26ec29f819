// æµ‹è¯•æ€§èƒ½ç›‘æŽ§å’Œè¶…æ—¶æ£€æµ‹
const testStartTime = new Date();
beforeEach(() => {
    testStartTime.setTime(Date.now());
});
afterEach(() => {
    const testDuration = Date.now() - testStartTime.getTime();
    // æ£€æµ‹æ…¢æµ‹è¯• (>10ç§’)
    if (testDuration > 10000) {
        console.warn(`ðŸŒ Slow test detected: ${expect.getState().currentTestName} took ${testDuration}ms`);
    }
    // æ£€æµ‹æ½œåœ¨çš„æ— é™å¾ªçŽ¯ (>30ç§’)
    if (testDuration > 30000) {
        console.error(`ðŸš¨ Potential infinite loop detected: ${expect.getState().currentTestName} took ${testDuration}ms`);
        throw new Error(`Test timeout: ${expect.getState().currentTestName} exceeded 30 seconds`);
    }
    // é‡ç½®æ‰€æœ‰mock
    jest.clearAllMocks();
    // æ£€æŸ¥æ˜¯å¦æœ‰æœªå¤„ç†çš„å¼‚æ­¥æ“ä½œ
    try {
        if (jest.getTimerCount() > 0) {
            console.warn(`âš ï¸ Warning: ${jest.getTimerCount()} timers still active after test: ${expect.getState().currentTestName}`);
        }
    }
    catch (error) {
        // fake timerså¯èƒ½æœªå¯ç”¨ï¼Œå¿½ç•¥é”™è¯¯
    }
});
afterAll(async () => {
    // æ¸…ç†æµ‹è¯•èµ„æº
    jest.clearAllMocks();
}, 5000);
// è‡ªå®šä¹‰åŒ¹é…å™¨
expect.extend({
    toBeValidDate(received) {
        const pass = received instanceof Date && !Number.isNaN(received.getTime());
        if (pass) {
            return {
                message: () => `expected ${received} not to be a valid date`,
                pass: true,
            };
        }
        else {
            return {
                message: () => `expected ${received} to be a valid date`,
                pass: false,
            };
        }
    },
    toBeValidUUID(received) {
        const uuidRegex = /^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$/i;
        const pass = typeof received === 'string' && uuidRegex.test(received);
        if (pass) {
            return {
                message: () => `expected ${received} not to be a valid UUID`,
                pass: true,
            };
        }
        else {
            return {
                message: () => `expected ${received} to be a valid UUID`,
                pass: false,
            };
        }
    },
});
// å†…å­˜ä½¿ç”¨ç›‘æŽ§
if (process.env.CI) {
    const initialMemory = process.memoryUsage();
    const memoryThreshold = 500 * 1024 * 1024; // 500MB
    afterEach(() => {
        const currentMemory = process.memoryUsage();
        const memoryIncrease = currentMemory.heapUsed - initialMemory.heapUsed;
        if (memoryIncrease > memoryThreshold) {
            console.warn(`Warning: Memory usage increased by ${Math.round(memoryIncrease / 1024 / 1024)}MB`);
        }
    });
}
